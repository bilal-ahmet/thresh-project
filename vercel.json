{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js",
      "headers": {
        "Permissions-Policy": "interest-cohort=()"
      }
    }
  ],
  "env": {
    "NODE_ENV": "production"
  }
}