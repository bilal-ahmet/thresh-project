{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ],
  "env": {
    "NODE_ENV": "production",
    "SESSION_SECRET": "thresh-project-session-secret-key-2024-vercel"
  },
  "functions": {
    "index.js": {
      "maxDuration": 10
    }
  }
}